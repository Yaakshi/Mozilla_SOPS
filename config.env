# Common settings
APP_NAME=my_awesome_app
APP_VERSION=1.0
LOG_LEVEL=INFO

# Production environment
PROD_DB_HOST=prod-db.example.com
PROD_DB_PORT=5432
PROD_DB_NAME=prod_database
